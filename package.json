		"deploy:dev": "npm run compile && npm publish --tag dev"
	},
	"dependencies": {
		"@types/node": "^13.5.3",
		"last-hit-workspace-extension": "^0.9.11",
		"typescript": "^3.7.5"
	},
	"devDependencies": {
		"gulp": "^4.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^6.0.0-alpha.1"
	}
}
