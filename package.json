{
	"name": "last-hit-matrix-workspace-extension",
	"version": "0.0.3",
	"description": "workspace extension for last-hit-matrix",
	"main": "dist/index.js",
	"repository": "https://github.com/last-hit-aab/last-hit-matrix-workspace-extension.git",
	"author": "last-hit-team",
	"license": "MIT",
	"scripts": {
		"compile": "gulp",
		"update:dev": "yarn add last-hit-workspace-extension@dev",
